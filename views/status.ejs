<!DOCTYPE html>
<html>
  <head>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx"
      crossorigin="anonymous"
    />
    
  </head>
  <body>
    <header>
        <%- include('./partials/header'); %>
    </header>
    <main>
          <%- include('./partials/dashboardf'); %>
          <div style="width:600px;margin-left:350px;margin-top: 50px;padding-left: 10px;">
            
            <table border="3">
              <tr>
                <th style="width:90px">lid</th>
                <th style="width:120px">From-date</th>
                <th style="width:120px">To-date</th>
                <th style="width:120px">Status</th>
                <th style="width:120px">More</th>
              </tr>
            
            <% 
            msg.forEach((i)=>{ %>
              <tr>
                <td ><%= i.lid%></td>
                <td><%= i.fromdate %></td>
                <td><%= i.todate%></td>
                <td><%= i.status%></td>
                <%
                var url2="http://localhost:3000/users/facultyreport/"+i.lid;
                %>
                <%if(i.status=="Accepted"){%>
                  <td><a href="<%=url2%>" target="_blank">Download CL</a></td>
                <%}%>
              </tr>
              <% 
            })
            %>
          </table>        
          </div>         
    </main>  
  </body>
</html>