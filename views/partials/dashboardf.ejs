<!DOCTYPE html>
<html>
<head>
  <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx"
      crossorigin="anonymous"
    />
<style>
  
ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #333;
}

li {
  float: left;
}

li a {
  display: block;
  color: white;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

li a:hover {
  background-color: #111;
}
li a:active{
  background-color: #333;
}
</style>
</head>
<body>
<ul>
<%
var url="/users/page2";
%>

<% var url1="/users/status"
   var url2="/logout"
%>

  <li><a class="active" href='<%=url%>'>Apply Leave</a></li>
  <li><a class="active" href='<%=url1%>'>Status</a></li>
  <li><a class="active" href='<%=url2%>'>Log Out</a></li>
</ul>
<%if(msg1.length!=0){%>
  <div style="margin-top: 150px;" align="center">
    <div class="card" align="center" style="width: 450px;height:150px;padding-top: 40px;color: rgb(78, 29, 29);font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;">
      <b><%=msg1%></b> <br>
     <b><%=msg2%></b> 
    </div>
  </div>
<%}%>

</body>
</html>